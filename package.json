{
    "name": "fatcher",
    "description": "A composable HTTP request library witch base on fetch API.",
    "keywords": [
        "fetch",
        "http",
        "request",
        "middlewares"
    ],
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "browser": "dist/fatcher.min.js",
    "unpkg": "dist/fatcher.min.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=17.5.0"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "rimraf dist && rollup -c rollup.config.ts -w",
        "build": "rimraf dist && rollup -c rollup.config.ts"
    },
    "dependencies": {
        "@fatcherjs/utils-shared": "^0.1.0"
    },
    "devDependencies": {
        "@fansy/eslint-config": "^1.0.0",
        "@fansy/prettier-config": "^1.0.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "esbuild": "^0.14.29",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-dts": "^4.2.0",
        "rollup-plugin-esbuild": "^4.8.2"
    }
}
