        "@types/jest": "^28.1.1",
        "@types/node": "^18.0.0",
        "esbuild": "^0.15.1",
        "fast-glob": "^3.2.11",
        "fs-extra": "^10.1.0",
        "jest": "^28.1.0",
        "jest-fetch-mock": "^3.0.3",
